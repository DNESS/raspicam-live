    "socketio",
    "react"
  ],
  "author": "wuct",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wuct/raspicam-live/issues"
  },
  "homepage": "https://github.com/wuct/raspicam-live",
  "dependencies": {
    "socket.io": "^1.2.1",
    "socket.io-client": "^1.2.1",
    "express": "^4.11.0",
    "ejs": "^2.2.1",
    "react": "^0.12.2"
  },
  "devDependencies": {
    "jsx-loader": "^0.12.2",
    "webpack": "^1.5.1",
    "parallelshell": "^1.0.3"
  }
}
