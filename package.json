{
  "name": "raspicam-live",
  "version": "1.0.0",
  "description": "It's a live streaming webcam built around Raspberry, Node.js, Socket.io and React.",
  "main": "server/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack-dev-server -c --inline",
    "clean": "rm -r -f web/dist/*",
    "build": "npm run clean && webpack",
    "start": "node server/server",
    "start:watch": "parallelshell 'NODE_ENV=dev node server/server' 'npm run dev'"
    
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wuct/raspicam-live.git"
  },
  "keywords": [
    "raspberry",
